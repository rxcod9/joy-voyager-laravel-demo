{
    "name": "joy/voyager-laravel-demo",
    "type": "project",
    "description": "Voyager Laravel Demo",
    "keywords": [
        "framework",
        "laravel",
        "joy",
        "voyager",
        "demo"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "joy/voyager-api": "^1.0",
        "joy/voyager-datatable": "^1.0",
        "joy/voyager-export": "^1.0",
        "joy/voyager-import": "^1.0",
        "joy/voyager-relations-table": "^1.0",
        "laravel/framework": "~6.0|~7.0|~8.0",
        "laravel/tinker": "^2.7",
        "symfony/filesystem": "^5.4|^6.0",
        "tcg/voyager": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.17",
        "fakerphp/faker": "^1.19",
        "friendsofphp/php-cs-fixer": "^3.7",
        "henzeb/composer-link": "^0.0.3@alpha",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "henzeb/composer-link": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
    }
}
