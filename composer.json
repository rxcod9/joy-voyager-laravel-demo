{
    "name": "joy/voyager-laravel-demo",
    "type": "project",
    "description": "Voyager Laravel Demo",
    "keywords": [
        "framework",
        "laravel",
        "joy",
        "voyager",
        "demo"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.4",
        "guzzlehttp/guzzle": "7.3.0",
        "joy/voyager-api": "dev-main",
        "joy/voyager-datatable": "dev-main",
        "joy/voyager-relations-table": "dev-main",
        "laravel/framework": "~6.0|~7.0|~8.0",
        "laravel/tinker": "2.6.1",
        "tcg/voyager": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.6.2",
        "facade/ignition": "2.10.2",
        "fakerphp/faker": "1.15.0",
        "friendsofphp/php-cs-fixer": "3.0.0",
        "laravel/sail": "1.8.6",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "5.11.0",
        "phpunit/phpunit": "9.5.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
